{
	"$schema": "https://json.schemastore.org/electron-builder",
	"appId": "com.company-name.app-name",
	"productName": "ReactElectronSetup",
	"files": [
		"dist-electron/**/*",
		"dist-ui/**/*"
	],
	"extraResources": [
		"src/assets/**"
	],
	"icon": "./desktopIcon.png",
	"mac": {
		"target": "dmg"
	},
	"linux": {
		"target": "AppImage",
		"category": "Utility"
	},
	"win": {
		"target": [
			"msi",
			"nsis",
			"portable"
		]
	},
	"deb": {},
	"dmg": {},
	"msi": {},
	"nsis": {
		"oneClick": false,
		"perMachine": false,
		"allowElevation": false,
		"allowToChangeInstallationDirectory": false,
		"deleteAppDataOnUninstall": true
	}
}